{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.AspNetCore.HttpLogging": "Information",
      "CarvedRock": "Debug"
    },
    "Console": {
      "FormatterName": "json",
      "FormatterOptions": {
        "SingleLine": true,
        "IncludeScopes": true,
        "TimestampFormat": "HH:mm:ss ",
        "UseUtcTimestamp": true,
        "JsonWriterOptions": {
          "Indented": true
        }
      }
    },
    "Debug": {
      "LogLevel": {
        "Default": "Critical"
      }
    }
  },
  "EreborApi": {
    "CorsAllowUrl": "*"
  },
  "seqLink": "http://localhost:5341",
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "EreborContext": "Server=(localdb)\\mssqllocaldb;Database=Erebor;Trusted_Connection=True;MultipleActiveResultSets=true"
  },
  "AiDeveloperProMax": {
    "LanguageClient": {
      "endpoint": "https://languageexperiment.cognitiveservices.azure.com/"
    },
    "TextTranslationClient": {
      "endpoint": "https://api.cognitive.microsofttranslator.com/",
      "region": "eastus"
    },
    "DocumentTranslationClient": {
      "endpoint": "https://translatorexperiment.cognitiveservices.azure.com/"
    }
  }
}
